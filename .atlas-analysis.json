    "GET /api/v1/plans - List pricing plans",
    "POST /api/v1/events - Ingest usage events",
    "GET /api/v1/usage - Query usage data",
    "gRPC AddonsService - Manage add-ons",
    "gRPC BillableMetricsService - Configure billable metrics",
